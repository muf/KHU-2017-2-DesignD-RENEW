<Window x:Class="SoccerTradingSystem.View.MakeGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SoccerTradingSystem.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MakeGameWindow" Height="600" Width="800"
        Loaded="OnWindowLoad">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="498*"/>
            <RowDefinition Height="71*"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="0" NavigationUIVisibility="Hidden"  Name="content_frame" Margin="10"/>
        <Button x:Name="NextBtn" Content="다음" HorizontalAlignment="Left" Margin="692,24,0,0" Grid.Row="1" VerticalAlignment="Top" Width="74" Click="NextBtn_Click"/>
        <Button x:Name="PrevBtn" Content="이전" HorizontalAlignment="Left" Margin="35,24,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Click="PrevBtn_Click"/>
        <Button x:Name="RegBtn" Content="등록" HorizontalAlignment="Left" Margin="608,24,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Click="RegBtn_Click"/>
    </Grid>
</Window>
