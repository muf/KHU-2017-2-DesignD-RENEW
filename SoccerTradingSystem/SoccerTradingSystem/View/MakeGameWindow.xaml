<Window x:Class="SoccerTradingSystem.View.MakeGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SoccerTradingSystem.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MakeGameWindow" Height="500" Width="500"
        Loaded="OnWindowLoad">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="498*"/>
            <RowDefinition Height="71*"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="0" NavigationUIVisibility="Hidden"  Name="content_frame" Margin="10"/>
        <Button Style="{StaticResource SuccessButton}" x:Name="NextBtn" Content="다음" HorizontalAlignment="Left" Margin="391,10,0,0" Grid.Row="1" VerticalAlignment="Top" Width="74" Click="NextBtn_Click" RenderTransformOrigin="-2.855,0.456"/>
        <Button Style="{StaticResource SuccessButton}" x:Name="PrevBtn" Content="이전" HorizontalAlignment="Left" Margin="28,10,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Click="PrevBtn_Click"/>
        <Button Style="{StaticResource PrimaryButton}" x:Name="RegBtn" Content="등록" HorizontalAlignment="Left" Margin="390,10,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Click="registerGame"/>
    </Grid>
</Window>
